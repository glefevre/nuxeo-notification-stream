<component name="org.nuxeo.ecm.platform.notification.stream">
  <extension point="logConfig" target="org.nuxeo.runtime.stream.service">
    <logConfig name="notification">
    </logConfig>
    <logConfig name="subscriptions">
    </logConfig>
    <logConfig name="notificationSettings">
    </logConfig>
  </extension>

  <extension target="org.nuxeo.runtime.stream.service" point="streamProcessor">
    <streamProcessor name="notification"
                     class="org.nuxeo.ecm.platform.notification.processors.NotificationProcessor"
                     logConfig="notification">
    </streamProcessor>
    <streamProcessor name="subscriptions"
                     class="org.nuxeo.ecm.platform.notification.processors.SubscriptionsProcessor"
                     logConfig="notification">
    </streamProcessor>
    <streamProcessor name="notificationSettings"
                     class="org.nuxeo.ecm.platform.notification.processors.NotificationSettingsProcessor"
                     logConfig="notificationSettings">
    </streamProcessor>
  </extension>
</component>